{
  "name": "music-player",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "fuse.js": "^3.3.0",
    "immutable": "4.0.0-rc.9",
    "lodash": "^4.17.10",
    "music-metadata": "^2.4.2",
    "normalize.css": "^8.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "web-streams-node": "steelbrain/web-streams-node#8a5910a911"
  },
  "scripts": {
    "start": "pundle --dev.singlepage --dev.static ./public::/",
    "build": "NODE_ENV=production pundle",
    "postinstall": "link-tilde",
    "lint": "eslint src && prettier --list-different src/**/*.js && flow check"
  },
  "tildeDirectory": "./src",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint-config-steelbrain": "^7.0.0-beta1",
    "flow-bin": "^0.79.1",
    "less": "^3.8.1",
    "link-tilde": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "pundle-cli": "^3.0.0-beta8",
    "pundle-preset-default": "^3.0.0-beta8"
  },
  "babel": {
    "plugins": [
      [
        "babel-plugin-transform-inline-environment-variables",
        {
          "include": [
            "APP_ID",
            "API_KEY",
            "CLIENT_ID"
          ]
        }
      ],
      "react-hot-loader/babel",
      "@babel/syntax-dynamic-import",
      "@babel/proposal-class-properties",
      "@babel/proposal-optional-chaining",
      [
        "module-resolver",
        {
          "alias": {
            "~": "./src"
          }
        }
      ]
    ],
    "presets": [
      "@babel/flow",
      [
        "@babel/env",
        {
          "exclude": [
            "transform-regenerator"
          ]
        }
      ],
      "@babel/react"
    ]
  }
}
